package org.eclipse.emf.eson.example.library

class Library {
    String name	
    contains Book[] books
    contains Writer[] writers
}

class Book {
    String name 
	int pages
	BookCategory category
	refers Writer author
	refers Book[] sequels
}

class Writer {
    String name 
	// TODO add int age
}

enum BookCategory {
	None, Mystery, SciFi, Biography
}
